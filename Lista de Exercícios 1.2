package fat.iap;

public class exercicio2 {
    public static void main(String[] args) {

        double a = 8;
        double b = 9;
        double c = 7;

        double x = 4;
        double y = 5;
        double z = 6;

        System.out.println("A média aritmética dos números 8,9 e 7 é: " + (a + b + c) / 3);
        System.out.println("A média aritmética dos números 4, 5 e 6 é: " + (x + y + z) / 3);
        System.out.println("A soma das duas médias é: " + (x + y + z + a + b + c) / 6);
    }
}
